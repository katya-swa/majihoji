<div class="container">
  <%= search_form_for @search  do |f| %>
    <div class="form-group">
      <%= f.label :name_cont, "ユーザー検索" %>
      <%= f.text_field :name_cont, class: "form-control" %>
    </div>
    <div class="actions"><%= f.submit "Search" %></div>
  <% end %>

  <h1>ユーザー</h1>
    <% if @result.any? %>
      <%= render @result %>
    <% end %>
    <% if @result.nil? %>
      <%= render @users %>
    <% end %>
    <%= paginate @users %>
</div>